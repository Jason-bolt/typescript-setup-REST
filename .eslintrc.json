{
  "parser": "@typescript-eslint/parser",
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "parserOptions": {
    "ecmaVersion": "es6",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "error",
    "@typescript-eslint/no-unused-vars": ["error"],
    "indent": ["error", 2]
  }
}
